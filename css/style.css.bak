/*swiper*/
.swiper-container-horizontal>.swiper-pagination-bullets, .swiper-pagination-custom, .swiper-pagination-fraction{
   bottom:6%;
    width: 80%;
	height:5px;
    background: #f4f4f4;
	border-radius:30px;
    transform: translateX(50%);
    right: 50%;
    left: auto;
 }
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet{
    height:5px;
    display: inline-block;
    background: #f4f4f4;
    opacity:0 !important;
    width:33.333%;
    border-radius:30px;
	margin:0px;
 }
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet-active{
 background:#3b3b3b;
 opacity:1 !important;
 outline:none;
 border:none;
}

/*팝업*/
/*.b-modal.__b-popup1__{background:transparent !important;}*/
.layer_pop {display:none; border:none; margin:0; padding:5px; background:none; z-index:300;}
.layer_pop.pop100{width:100%; height:100%; padding:0px;}
.layer_pop .b-close {position:absolute;top:20px;left:20px; font-weight:bold;cursor:pointer; z-index:400;}
.layer_pop .close_circle{
 width:56px;
 height:56px;
 background:#fff;
 border-radius:50%;
 padding:0px;
 left: auto;
 top: 25px;
 right: 50%;
 transform: translateX(50%);
}
.b-close img{width:100%; height:auto;}
.layer_pop .popupCont {margin:0;padding:20px;text-align:center;border:0;width:300px; background:#fff;}
.pop_join .popupCont{padding:75px 20px 25px;}
.layer_pop.pop100 .popupCont{width:100%; height:100%; background:#f7f7fa;}
.popupCont .caution{background:#fff; padding:10px 0px 15px; text-align:center; font-size:14px; color:#191919; position: relative;}
.popupCont .caution h3{font-size:20px; font-weight:700; padding:3px 0px; line-height: 1.5; letter-spacing: 0px;}
.popupCont .caution h3.failed{color:#d0274e;}
.popupCont .caution .comment{font-size:15px; line-height:1.6; padding: 1px 0px; letter-spacing: -0.5px;}
.popupCont .caution .comment .green{color:#00b5c3;}
.popupCont .caution .non{padding:12px 0px;}
.popupCont .line{
  width:100%;
  display:flex;
  align-items: center;
  justify-content: center;
  background:#fff;
  }
.popupCont .line a{
 display:block; 
 width:100%; 
 height:40px; 
 line-height:41px;
 font-size:15px; 
 text-align:center;
 overflow:hidden; 
 font-weight:400;
 background:#fff;
 border:1px solid #ccc;
 color:#191919;
 letter-spacing: 0.5px;
 }
.popupCont .line .cancel{
 width:40%;
 background:#fff;
 margin:0px 2%;
}
.popupCont .line .okay{
 width:40%;
 background:#ff9b27;
 border:none;
 color:#fff;
  margin:0px 2%;
}

